        location = []
        for emp in mydata:
            empDict = {
                "number": emp[0],
                "name": emp[1],
                "address": emp[2],
                "latitude": emp[3],
                "longitude": emp[4]}
            location.append(empDict)
        for i in location:
            print(location[i][i])
    
    return jsonify(location)
    
    
